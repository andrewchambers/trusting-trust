#!/bin/sh
# dependency-hash (used by redo) @DEPHASH@
set -eu

DIR=$(dirname "$(realpath "$0")")

exec env -i \
	MES_ARENA=20000000 \
	MES_MAX_ARENA=20000000 \
	MES_STACK=6000000 \
	MES_PREFIX="$DIR/.././mes-m2" \
	GUILE_LOAD_PATH="$DIR/../mes-m2/module:$DIR/../nyacc/module" \
	PATH="$DIR/../../stage0/bin" \
	"$DIR/mes-m2" \
	-e main \
	"$DIR/../mes-m2/scripts/mescc.scm" \
	"$@"

